{
  "name": "reactdemo",
  "version": "1.0.0",
  "description": "框架搭建测试",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack --watch --devtool eval --progress --colors --hot --inline --content-base ./dist",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixyao1985/react-demo"
  },
  "keywords": [
    "redux"
  ],
  "author": "Felix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/felixyao1985/react-demo/issues"
  },
  "homepage": "https://github.com/felixyao1985/react-demo#readme",
