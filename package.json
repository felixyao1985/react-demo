{
  "name": "reactdemo",
  "version": "1.0.0",
  "description": "框架搭建测试",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack --watch --devtool eval --progress --colors --hot --inline --content-base ./dist",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
     "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    }, 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixyao1985/react-demo"
  },
  "keywords": [
    "redux"
  ],
  "author": "Felix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/felixyao1985/react-demo/issues"
  },
  "homepage": "https://github.com/felixyao1985/react-demo#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "better-npm-run": "0.0.8",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "css-loader": "^0.28.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "debug": "^2.2.0",
    "rimraf": "^2.5.1",
    "fs-extra": "^0.30.0",
    "ip": "^1.1.2",
    "cssnano": "^3.3.2"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-cookie": "^0.4.6",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "classnames": "^2.2.5",
    "fbjs": "^0.8.2",
    "normalize.css": "^4.1.1",
    "object-assign": "^4.1.0",
    "axios": "^0.16.2",
    "babel-preset-stage-3": "^6.24.1"
  }
}
